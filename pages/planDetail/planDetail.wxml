<view>
  <view class='navigation'>
    <view class='startBar' style='height:{{startBarHeight}}px'></view>
    <view class='navgation' style='height:{{navgationHeight}}px'>
      <view class="wenzi" bindtap="fanhui">返回</view>
    </view>
  </view>
  <view style="margin-top:{{startBarHeight+navgationHeight}}px">
    <view class="title">计划信息</view>
    <van-collapse value="{{ activeNames }}" bind:change="onChange">
      <van-collapse-item title="有赞微商城" wx:for="{{planList}}" data-id="{{item.plannumber}}" name="{{item.plannumber}}">
        <view slot="title">项目名称:{{item.planname}} 生产时间:{{item.plantime}}</view>
        <view wx:for="{{preproductList}}">
          <view>物料编码：{{item.materialcode}} 构件号：{{item.preproductid}}</view>
          <view>物料名称：{{item.materialname}} 质检员：{{item.qc}}</view>
          <view>打印次数：{{item.print}} </view>
        </view>
      </van-collapse-item>
    </van-collapse>
  </view>
</view>